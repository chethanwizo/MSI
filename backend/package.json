{
  "name": "mis-analytics-backend",
  "version": "1.0.0",
  "description": "Backend API for MIS Analytics Platform",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "prisma generate && prisma db push --accept-data-loss",
    "postbuild": "npm run seed",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "seed": "node src/seed.js",
    "setup": "npm run db:generate && npm run db:push && npm run seed",
    "render:setup": "npm run build && npm run seed"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}